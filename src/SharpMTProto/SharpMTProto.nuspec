<?xml version="1.0" encoding="utf-8"?>

<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>SharpMTProto</id>
    <version>$version$</version>
    <title>SharpMTProto</title>
    <authors>Alexander Logger</authors>
    <owners>Alexander Logger</owners>
    <licenseUrl>https://github.com/inTagger/SharpMTProto/blob/master/LICENSE.md</licenseUrl>
    <projectUrl>https://github.com/inTagger/SharpMTProto</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Portable class library contains MTProto Mobile Protocol (https://core.telegram.org/mtproto) implementation.
      Supported platforms:

      .NET Framework 4.5
      .NET for Windows Store apps
      .NET for Windows Phone 8 apps
      Portable Class Libraries
    </description>
    <dependencies>
      <dependency id="SharpTL" version="0.4.2" />
      <dependency id="Catel.Core" version="3.8.1" />
      <dependency id="Rx-Main" version="2.2.2" />
      <dependency id="Nito.AsyncEx" version="2.1.3" />
      <dependency id="Raksha" version="0.1.1" />
    </dependencies>
  </metadata>
  <files>
    <file src="SharpMTProto.PCL\bin\$config$\SharpMTProto.dll" target="lib\portable-net45+wp8+win8\" />
    <file src="SharpMTProto.PCL\bin\$config$\SharpMTProto.pdb" target="lib\portable-net45+wp8+win8\" />
    <file src="SharpMTProto.PCL\bin\$config$\SharpMTProto.XML" target="lib\portable-net45+wp8+win8\" />
    <file src="SharpMTProto.PCL\bin\$config$\SharpMTProto.dll" target="lib\net45\" />
    <file src="SharpMTProto.PCL\bin\$config$\SharpMTProto.pdb" target="lib\net45\" />
    <file src="SharpMTProto.PCL\bin\$config$\SharpMTProto.XML" target="lib\net45\" />
    <file src="SharpMTProto.PCL\**\*.cs" target="src\SharpMTProto.PCL\" exclude="**\obj\**" />
    <file src="SharpMTProto.Extra.NET45\bin\$config$\SharpMTProto.Extra.dll" target="lib\net45\" />
    <file src="SharpMTProto.Extra.NET45\bin\$config$\SharpMTProto.Extra.pdb" target="lib\net45\" />
    <file src="SharpMTProto.Extra.NET45\bin\$config$\SharpMTProto.Extra.XML" target="lib\net45\" />
    <file src="SharpMTProto.Extra.NET45\**\*.cs" target="src\SharpMTProto.Extra.NET45\" exclude="**\obj\**" />
  </files>
</package>